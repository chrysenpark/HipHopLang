open "testing/images/test-color.jpg" as img
apply filtercolor 50 50 110 255 255 130 to img
save img as "testing/output/test-filtercolor.jpg"
open "testing/images/test-color.jpg" as img2
apply crop -0.5 0 -0.5 0 to img2
save img2 as "testing/output/test-crop.jpg"
save img2 as "testing/output/hi/path.jpg"